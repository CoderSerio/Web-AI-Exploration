#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/code/fe/programs/masterpiece/node_modules/.pnpm/@umijs+max@4.1.2_@babel+core@7.24.0_@types+react-dom@18.2.21_@types+react@18.2.64_dva@2.5.0-b_ao5mwtbo7a7pirg4wdwiuo6aaa/node_modules/@umijs/max/bin/node_modules:/mnt/d/code/fe/programs/masterpiece/node_modules/.pnpm/@umijs+max@4.1.2_@babel+core@7.24.0_@types+react-dom@18.2.21_@types+react@18.2.64_dva@2.5.0-b_ao5mwtbo7a7pirg4wdwiuo6aaa/node_modules/@umijs/max/node_modules:/mnt/d/code/fe/programs/masterpiece/node_modules/.pnpm/@umijs+max@4.1.2_@babel+core@7.24.0_@types+react-dom@18.2.21_@types+react@18.2.64_dva@2.5.0-b_ao5mwtbo7a7pirg4wdwiuo6aaa/node_modules/@umijs/node_modules:/mnt/d/code/fe/programs/masterpiece/node_modules/.pnpm/@umijs+max@4.1.2_@babel+core@7.24.0_@types+react-dom@18.2.21_@types+react@18.2.64_dva@2.5.0-b_ao5mwtbo7a7pirg4wdwiuo6aaa/node_modules:/mnt/d/code/fe/programs/masterpiece/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/code/fe/programs/masterpiece/node_modules/.pnpm/@umijs+max@4.1.2_@babel+core@7.24.0_@types+react-dom@18.2.21_@types+react@18.2.64_dva@2.5.0-b_ao5mwtbo7a7pirg4wdwiuo6aaa/node_modules/@umijs/max/bin/node_modules:/mnt/d/code/fe/programs/masterpiece/node_modules/.pnpm/@umijs+max@4.1.2_@babel+core@7.24.0_@types+react-dom@18.2.21_@types+react@18.2.64_dva@2.5.0-b_ao5mwtbo7a7pirg4wdwiuo6aaa/node_modules/@umijs/max/node_modules:/mnt/d/code/fe/programs/masterpiece/node_modules/.pnpm/@umijs+max@4.1.2_@babel+core@7.24.0_@types+react-dom@18.2.21_@types+react@18.2.64_dva@2.5.0-b_ao5mwtbo7a7pirg4wdwiuo6aaa/node_modules/@umijs/node_modules:/mnt/d/code/fe/programs/masterpiece/node_modules/.pnpm/@umijs+max@4.1.2_@babel+core@7.24.0_@types+react-dom@18.2.21_@types+react@18.2.64_dva@2.5.0-b_ao5mwtbo7a7pirg4wdwiuo6aaa/node_modules:/mnt/d/code/fe/programs/masterpiece/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/max.js" "$@"
else
  exec node  "$basedir/../../bin/max.js" "$@"
fi
